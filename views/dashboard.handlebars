<h2>Your Posts</h2>
{{#each posts as |post|}}
  <a href='/api/dashboard/edit/{{post.id}}'>
    <div class='card'>
      <div class='car-header'>
        <h1>{{post.title}}</h1>
        <p>Posted by {{post.user.user_name}} on {{format_date created_at}}</p>
      </div>
      <div class='card-text'>
        <p>
          {{post.post}}
        </p>
      </div>
    </div>
  </a>
{{/each}}